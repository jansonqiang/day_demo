    package="com.example.qiang.d_scrollview" >


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"/>
        <activity android:name=".MultiScreenActivity"/>
        <activity android:name=".myTouch.MyTouchActivity"/>
        <activity android:name=".myTouch.MyScrollerActivity"/>
        <activity android:name=".measure.MeasureActivity"/>
        <activity android:name=".myRxJava.RxJavaActivity"/>
        <activity android:name=".myhttp.HttpActivity"/>
        <activity android:name=".myhttp.MyOkHttpActivity"/>
        <activity android:name=".retrolambda.MyRetrolambdaActivity"/>

    </application>




</manifest>
